{
  "name": "@forthright/vile-htmllint",
  "version": "0.2.12",
  "description": "A vile plugin for htmllint",
  "license": "MPL-2.0",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "compile": "babel -d lib -m common src",
    "compile-dev": "coffee -cbo .test test",
    "dev": "bin/dev",
    "test": "bin/test",
    "test-cov": "bin/test-cov",
    "update": "ncu -a",
    "vile": "vile p -usi --nodecorations",
    "release-it": "release-it"
  },
  "dependencies": {
    "bluebird": "^3.4.3",
    "cjson": "^0.4.0",
    "htmllint": "^0.4.0",
    "lodash": "^4.15.0"
  },
