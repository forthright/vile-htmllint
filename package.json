{
  "name": "@brentlintner/vile-htmllint",
  "version": "0.1.1",
  "description": "A vile plugin for htmllint",
  "license": "MPL-2.0",
  "main": "lib/index.js",
  "scripts": {
    "postinstall": "babel -d lib -m common src",
