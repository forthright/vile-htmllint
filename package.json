    "@forthright/vile-eclint": "0.0.2",
    "@forthright/vile-eslint": "0.0.3",
    "@forthright/vile-fs-lint": "^0.1.1",
    "@forthright/vile-git": "^0.1.0",
    "@forthright/vile-language": "0.0.4",
    "@forthright/vile-license": "0.0.3",
