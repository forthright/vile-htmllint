    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coffee-script": "1.11.0",
    "istanbul": "^0.4.5",
    "mimus": "^0.2.2",
    "mocha": "^3.0.2",
    "npm-check-updates": ">=2.8.0",
    "release-it": "^2.4.3",
    "sinon": "^1.17.5",
